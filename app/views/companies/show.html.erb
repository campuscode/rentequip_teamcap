<h1><%= @company.name %></h1>
<table class='table table-striped'>
  <tr>
    <th>CNPJ</th>
    <td><%= @company.cnpj %></td>
  </tr>
  <tr>
    <th>Endereço</th>
    <td><%= @company.address %></td>
  </tr>
  <tr>
    <th>Email</th>
    <td><%= @company.mail %></td>
  </tr>
  <tr>
    <th>Telefone</th>
    <td><%= @company.phone %></td>
  </tr>
  <tr>
    <th>Razão Social</th>
    <td><%= @company.company_name %></td>
  </tr>
  <tr>
    <th>Contato</th>
    <td><%= @company.contact %></td>
  </tr>
</table>
<%= link_to 'Voltar', companies_path %>

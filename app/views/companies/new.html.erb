<section>
  <%= flash[:error] %>
  <p><%= form_for @company do |c| %></p>
    <div class="form-group">
      <%= c.label :name %>
      <%= c.text_field :name, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= c.label :cnpj %>
      <%= c.text_field :cnpj, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= c.label :address %>
      <%= c.text_field :address, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= c.label :mail %>
      <%= c.text_field :mail, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= c.label :phone %>
      <%= c.text_field :phone, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= c.label :company_name %>
      <%= c.text_field :company_name, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= c.label :contact %>
      <%= c.text_field :contact, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= c.submit 'Criar Empresa' %>
    </div>
  <% end %>
</section>
